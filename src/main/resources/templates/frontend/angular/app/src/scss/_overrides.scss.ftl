.p-panelmenu .p-panelmenu-icon {

	margin-right	: 0;
	order			: 1;
	margin-left		: auto;

}

.toast-container .ngx-toastr {

	box-shadow: none !important;

}

.fieldset-right {

	display			: block;
	float			: right;
	margin-top		: -4.5rem;
	margin-right	: -1rem;
	padding			: 0.5rem !important;

}

.p-menuitem-link-active {

	color			: var(--primary-color) !important;

}

.padded-table {

	td {

		padding: 0.8rem;

	}

}

p-breadcrumb {

	@include styleclass('mb-2');
	display			: block;

}

